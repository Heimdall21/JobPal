{
  "short_name": "JobPal",
  "name": "JobPal Extension",
  "version": "1.0",
   "manifest_version": 3,
   "action": {},
   "icons": {
       "16": "jobpal.png",
       "48": "jobpal.png",
       "128": "jobpal.png"
   },
   "permissions": [
        "activeTab",
        "storage",
        "scripting"
   ],
   "background": {
        "service_worker": "background.bundle.js"
  },
  "web_accessible_resources": [
     {
       "resources": ["jobpal.png"],
       "matches": ["<all_urls>"]
     }
  ]
}
